<div class="col-xs-11" id="accordion">
	<div class="panel panel-default">
		<div class="panel-heading">
			<h4 class="panel-title">
				<a data-toggle="collapse" data-parent="#accordion" href="#collapseParameters">Launch Parameters</a>
			</h4>
		</div>
		<div id="collapseParameters" class="panel-collapse collapse">
			<div class="panel-body">
				<table class="table table-striped table-condensed">
					<thead>
						<tr>
							<th>Name</th>
							<th>Value</th>
						</tr>
					</thead>
					<tbody>
						@foreach( KeyValuePair<string, string> param in ViewBag.LaunchParameters ) {
							<tr>
								<td>@param.Key</td>
								<td>@param.Value</td>
							</tr>
						}
					</tbody>
				</table>
			</div>
		</div>
	</div>
</div>